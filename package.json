{
  "name": "c-gen-backend",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "npm run start:server",
    "start:server": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test,clients}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "njode --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "installDeps": "npm install && cd clients && npm install",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@apollo/federation-internals": "^2.9.3",
    "@apollo/gateway": "^2.9.3",
    "@apollo/server": "^4.11.2",
    "@langchain/core": "^0.3.24",
    "@langchain/openai": "^0.3.14",
    "@nestjs/apollo": "^12.2.1",
    "@nestjs/common": "^10.4.8",
    "@nestjs/core": "^10.4.8",
    "@nestjs/graphql": "^12.2.1",
    "@nestjs/mapped-types": "*",
    "@nestjs/platform-express": "^10.4.13",
    "@nestjs/swagger": "^8.0.7",
    "@nestjs/throttler": "^6.2.1",
    "@nestjs/typeorm": "^10.0.2",
    "apollo-server-express": "^3.13.0",
    "app-root-path": "^3.1.0",
    "aws-sdk": "^2.1692.0",
    "cheerio": "^1.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "8.2.0",
    "graphql": "^16.9.0",
    "graphql-tools": "^9.0.3",
    "langchain": "^0.3.7",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "openai": "^4.77.0",
    "pg": "8.5.1",
    "reflect-metadata": "^0.2.2",
    "rimraf": "3.0.2",
    "rxjs": "^7.8.1",
    "shebang-regex": "^4.0.0",
    "swagger-ui-express": "^4.6.3",
    "ts-morph": "^24.0.0",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.7",
    "@nestjs/schematics": "^10.2.3",
    "@nestjs/testing": "^10.4.8",
    "@types/express": "4.17.11",
    "@types/jest": "26.0.21",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.9.1",
    "@types/superagent": "^8.1.9",
    "@types/supertest": "2.0.10",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "concurrently": "6.0.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "8.1.0",
    "eslint-plugin-prettier": "3.3.1",
    "jest": "^29.7.0",
    "lint-staged": "10.5.4",
    "prettier": "2.2.1",
    "simple-git-hooks": "2.2.0",
    "supertest": "6.1.3",
    "ts-jest": "^29.2.5",
    "ts-loader": "8.0.17",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "3.9.0",
    "typescript": "^5.6.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint"
    ]
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "echo \"[Husky] pre-commit\""
    }
  }
}
